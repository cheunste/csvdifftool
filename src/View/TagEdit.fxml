<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity"
            prefHeight="800.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/8.0.171">
    <left>
        <VBox prefHeight="800.0" prefWidth="238.0" BorderPane.alignment="CENTER">
            <children>
                <Label text="Tag Filter:"/>
                <TextField/>
                <Label text="Tag List:"/>
                <ListView prefHeight="696.0" prefWidth="198.0"/>
            </children>
            <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
            </padding>
        </VBox>
    </left>
    <top>
        <MenuBar BorderPane.alignment="CENTER">
            <menus>
                <Menu mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Close"/>
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Delete"/>
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                    <items>
                        <MenuItem mnemonicParsing="false" text="About"/>
                    </items>
                </Menu>
            </menus>
        </MenuBar>
    </top>
    <right>
        <VBox alignment="TOP_CENTER" prefHeight="775.0" prefWidth="186.0" BorderPane.alignment="CENTER">
            <children>
                <Button mnemonicParsing="false" text="Save"/>
                <JFXButton buttonType="RAISED" text="Force Copy"/>
                <HBox alignment="CENTER"/>
            </children>
        </VBox>
    </right>
    <center>
        <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
            <children>
                <HBox>
                    <children>
                        <Label text="Tag Name:"/>
                    </children>
                    <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </VBox.margin>
                </HBox>
                <HBox layoutX="10.0" layoutY="10.0" prefHeight="677.0" prefWidth="726.0">
                    <children>
                        <VBox fx:id="CommonDisplay">
                            <children>
                                <HBox>
                                    <Text>Command:</Text>
                                    <JFXComboBox fx:id="commandComboBox" promptText="Select Command"/>
                                </HBox>
                                <HBox>
                                    <Label>Variable internal ID</Label>
                                    <JFXTextField fx:id="variableInternalID" promptText="Value 1 to 300000"/>
                                </HBox>
                                <HBox>
                                    <Label>1st element of variable</Label>
                                    <JFXTextField fx:id="1stElementVariable"/>
                                </HBox>
                                <HBox>
                                    <Label>2st element of variable</Label>
                                    <JFXTextField fx:id="2ndElementVariable"/>
                                </HBox>
                                <HBox>
                                    <Label>3rd element of variable</Label>
                                    <JFXTextField fx:id="3rdElementVariable"/>
                                </HBox>
                                <HBox>
                                    <Label>4th element of variable</Label>
                                    <JFXTextField fx:id="4thElementVariable"/>
                                </HBox>
                                <HBox>
                                    <Label>5th element of variable</Label>
                                    <JFXTextField fx:id="5thElementVariable"/>
                                </HBox>
                                <HBox>
                                    <Label>6th element of variable</Label>
                                    <JFXTextField fx:id="6thElementVariable"></JFXTextField>
                                </HBox>
                                <HBox>
                                    <Label>7th element of variable</Label>
                                    <JFXTextField fx:id="7thElementVariable"></JFXTextField>
                                </HBox>

                            </children>
                        </VBox>
                        <VBox fx:id="CommandDisplay"/>
                    </children>
                    <padding>
                        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
                    </padding>
                </HBox>
            </children>
        </VBox>
    </center>
</BorderPane>
